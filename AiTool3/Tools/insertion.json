// insertions.json
{
  "name": "Insertions",
  "description": "A piece of code to be inserted, replacing the //! marker",
  "input_schema": {
    "type": "object",
    "properties": {
      "code": {
        "type": "string",
        "description": "The code to insert"
      },
      "explanation": {
        "type": "string",
        "description": "An explanation of the code insertion"
      },
    },
    "required": [ "code","explanation" ]
  }
}
